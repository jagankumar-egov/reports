    "project-task-index-v1": {
      "name": "project-task-index-v1",
      "description": "Individual tasks and activities within health projects with progress tracking",
      "tags": ["tasks", "activities", "project-management", "progress", "milestones"],
      "category": "project-management",
      "priority": 6,
      "dateFields": ["Data.startDate", "Data.dueDate", "Data.completedDate"],
      "primaryFields": ["Data.taskName", "Data.projectId", "Data.assignee", "Data.status"],
      "businessContext": {
        "domain": "Project Task Management",
        "dataSource": "Project Management System",
        "updateFrequency": "Real-time",
        "recordCount": 75000
      },
      "queryPatterns": [
        {
          "name": "overdue-tasks",
          "description": "Find tasks that are past their due date",
          "keywords": ["overdue", "delayed", "late", "past due"],
          "template": {
            "bool": {
              "must": [
                {
                  "range": {
                    "Data.dueDate": {
                      "lt": "now"
                    }
                  }
                },
                {
                  "terms": {
                    "Data.status.keyword": ["In Progress", "Not Started"]
                  }
                }
              ]
            }
          },
          "requiredFields": ["Data.dueDate", "Data.status"],
          "examples": [
            "show me overdue tasks",
            "find delayed activities",
            "tasks past their deadline"
          ]
        }
      ],
      "fields": {
        "Data.taskId": {
          "name": "Data.taskId",
          "type": "keyword",
          "description": "Unique identifier for the task",
          "aliases": ["task id", "activity id", "task number"],
          "examples": ["TASK-001", "ACT-2024-001"],
          "isSearchable": true,
