          "tags": ["category", "health", "classification"],
          "validValues": ["Maternal Health", "Child Health", "Infectious Disease", "Mental Health", "Nutrition", "Primary Care", "Emergency Care", "Preventive Care"]
        },
        "Data.createdDate": {
          "name": "Data.createdDate",
          "type": "date",
          "description": "Date when the project was created in the system",
          "aliases": ["created date", "creation date", "start date", "registered date"],
          "examples": ["2024-01-15", "2023-12-01", "2024-03-20"],
          "isSearchable": false,
          "isFilterable": true,
          "isAggregatable": true,
          "businessMeaning": "When the project was first registered in the tracking system",
          "tags": ["date", "timestamp", "creation"],
          "dateFormat": "yyyy-MM-dd"
        },
        "Data.lastUpdated": {
          "name": "Data.lastUpdated",
          "type": "date",
          "description": "Last modification timestamp for project data",
          "aliases": ["last updated", "modified date", "updated date", "last changed"],
          "examples": ["2024-01-25T10:30:00Z", "2024-01-20T14:15:00Z"],
          "isSearchable": false,
          "isFilterable": true,
          "isAggregatable": true,
          "businessMeaning": "Most recent time when project information was updated",
          "tags": ["date", "timestamp", "modification"],
          "dateFormat": "yyyy-MM-dd'T'HH:mm:ss'Z'"
        }
      }
