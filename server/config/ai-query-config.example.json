  "indexes": {
    "project-index-v1": {
      "name": "project-index-v1",
      "description": "Health project data including implementation details, status, and outcomes",
      "tags": ["health", "projects", "implementation", "outcomes", "monitoring"],
      "category": "health-data",
      "priority": 9,
      "dateFields": ["Data.createdDate", "Data.lastUpdated", "@timestamp"],
      "primaryFields": ["Data.projectName", "Data.status", "Data.implementingAgency", "Data.location"],
      "businessContext": {
        "domain": "Healthcare Project Management",
        "dataSource": "DHIS2 Health Project Tracker",
        "updateFrequency": "Daily",
        "recordCount": 15000
      },
      "queryPatterns": [
        {
          "name": "project-status-filter",
          "description": "Filter projects by their implementation status",
          "keywords": ["status", "completed", "ongoing", "failed", "pending"],
          "template": {
            "bool": {
              "must": [
                {
                  "term": {
                    "Data.status.keyword": "{{status_value}}"
                  }
                }
              ]
            }
          },
          "requiredFields": ["Data.status"],
          "examples": [
            "show me completed projects",
            "find all failed health projects",
            "list ongoing implementations"
          ]
        },
        {
          "name": "location-based-search",
          "description": "Search projects by geographical location",
          "keywords": ["location", "district", "state", "region", "area"],
          "template": {
            "bool": {
              "should": [
                {
                  "match": {
                    "Data.location.district": "{{location_value}}"
                  }
                },
                {
                  "match": {
                    "Data.location.state": "{{location_value}}"
                  }
                }
              ]
            }
          },
          "requiredFields": ["Data.location"],
          "examples": [
            "projects in Karnataka",
            "health initiatives in Bangalore district",
            "show me projects from southern region"
          ]
        }
      ],
      "fields": {
        "Data.projectId": {
          "name": "Data.projectId",
          "type": "keyword",
          "description": "Unique identifier for the health project",
          "aliases": ["project id", "id", "project number", "identifier"],
          "examples": ["PROJ-2024-001", "HP-KAR-001", "HEALTH-001"],
          "isSearchable": true,
          "isFilterable": true,
          "isAggregatable": true,
          "businessMeaning": "System-generated unique identifier for tracking projects",
          "tags": ["identifier", "primary-key"]
        },
        "Data.projectName": {
          "name": "Data.projectName",
          "type": "text",
          "description": "Full name of the health project or initiative",
          "aliases": ["project name", "title", "project title", "name"],
          "examples": ["Maternal Health Improvement Initiative", "Rural Healthcare Access Program", "COVID-19 Vaccination Drive"],
          "isSearchable": true,
          "isFilterable": false,
          "isAggregatable": false,
          "businessMeaning": "Official name of the health project as registered in DHIS2",
          "tags": ["name", "searchable", "primary-info"]
        },
        "Data.status": {
          "name": "Data.status",
          "type": "keyword",
          "description": "Current implementation status of the project",
          "aliases": ["status", "state", "condition", "progress", "phase"],
          "examples": ["Completed", "Ongoing", "Failed", "Pending", "Cancelled"],
          "isSearchable": true,
          "isFilterable": true,
          "isAggregatable": true,
          "businessMeaning": "Indicates the current state of project implementation",
          "tags": ["status", "categorical", "filter"],
          "validValues": ["Completed", "Ongoing", "Failed", "Pending", "Cancelled", "On Hold"]
        },
        "Data.implementingAgency": {
          "name": "Data.implementingAgency",
          "type": "keyword",
          "description": "Organization or agency responsible for project implementation",
          "aliases": ["implementing agency", "agency", "organization", "implementer", "responsible party"],
          "examples": ["Department of Health", "WHO", "UNICEF", "Local NGO", "State Health Mission"],
          "isSearchable": true,
          "isFilterable": true,
          "isAggregatable": true,
          "businessMeaning": "The primary organization accountable for executing the project",
          "tags": ["organization", "responsibility", "categorical"]
        },
        "Data.budget": {
          "name": "Data.budget",
          "type": "number",
          "description": "Total budget allocated for the project in USD",
          "aliases": ["budget", "funding", "cost", "allocation", "money"],
          "examples": ["50000", "250000", "1000000"],
          "isSearchable": false,
          "isFilterable": true,
          "isAggregatable": true,
          "businessMeaning": "Financial resources allocated for project completion",
          "tags": ["financial", "numeric", "aggregatable"]
        },
        "Data.location": {
          "name": "Data.location",
          "type": "object",
          "description": "Geographical information about project implementation area",
          "aliases": ["location", "place", "geography", "area", "region"],
          "examples": [],
          "isSearchable": true,
          "isFilterable": true,
          "isAggregatable": false,
          "businessMeaning": "Geographic scope and administrative boundaries of project implementation",
          "tags": ["location", "geographical", "nested"],
          "nestedFields": {
            "state": {
              "name": "Data.location.state",
              "type": "keyword",
              "description": "State or province where project is implemented",
              "aliases": ["state", "province"],
              "examples": ["Karnataka", "Tamil Nadu", "Kerala"],
              "isSearchable": true,
              "isFilterable": true,
              "isAggregatable": true,
              "businessMeaning": "Administrative state/province boundary",
              "tags": ["administrative", "geography"]
